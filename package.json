{
  "name": "react-gantt-chart",
  "version": "0.0.1",
  "description": "A React implementation of interactive gantt library from Frappe",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "npx rimraf dist && rollup -c -w --environment DEV",
    "build": "npx rimraf dist && rollup -c",
    "eslint": "eslint src --ext .jsx --ext .js",
    "eslint-fix": "eslint --fix src --ext .jsx --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vrajpal-jhala/react-gantt-chart.git"
  },
  "keywords": [
    "react",
    "gantt",
    "svg",
    "simple gantt",
    "project timeline",
    "interactive gantt",
    "project management"
  ],
  "author": "Vrajpal Jhala",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vrajpal-jhala/react-gantt-chart/issues"
  },
  "homepage": "https://github.com/vrajpal-jhala/react-gantt-chart",
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-replace": "^4.0.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.4",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.12",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "react": ">=16.2.0",
    "react-dom": ">=16.2.0"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md",
    "package.json"
  ],
  "eslintIgnore": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint"
    }
  }
}
